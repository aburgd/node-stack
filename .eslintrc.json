{
    "extends": [
        "airbnb",
        "plugin:flowtype/recommended"
    ],
    "plugins": [
        "flowtype",
        "compat"
    ],
    "rules": {
        "semi": [2, "never"],
        "no-unexpected-multiline": 2,
        "linebreak-style": [2, "windows"],
        "indent": ["error", 4],
        "compat/compat": 2
    },
    "env": {
        "jest": true
    }
}